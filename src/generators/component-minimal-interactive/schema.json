{
  "$schema": "http://json-schema.org/schema",
  "cli": "nx",
  "$id": "ComponentMinimalInteractive",
  "title": "",
  "type": "object",
  "properties": {
    "componentName": {
      "type": "string",
      "description": "",
      "$default": {
        "$source": "argv",
        "index": 0
      },
      "pattern": "^(?!.*[/])(?!.*[Cc]omponent).*$",
      "x-prompt": "Input the name of the component, e.g. 'id-input'. Do not append with '.component' '-component' or 'Component'.\n"
    }
  },
  "required": ["componentName"]
}
