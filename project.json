{
  "name": "nx-angular-generators",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "frontend/tools/nx-angular-generators/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/tools/nx-angular-generators",
        "main": "tools/nx-angular-generators/src/index.ts",
        "tsConfig": "tools/nx-angular-generators/tsconfig.lib.json",
        "assets": [
          "tools/nx-angular-generators/*.md",
          "tools/nx-angular-generators/*.npmrc",
          {
            "input": "./tools/nx-angular-generators/src",
            "glob": "**/!(*.ts)",
            "output": "./src"
          },
          {
            "input": "./tools/nx-angular-generators/src",
            "glob": "**/*.d.ts",
            "output": "./src"
          },
          {
            "input": "./tools/nx-angular-generators",
            "glob": "generators.json",
            "output": "."
          },
          {
            "input": "./tools/nx-angular-generators",
            "glob": "executors.json",
            "output": "."
          }
        ]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "tools/nx-angular-generators/**/*.ts",
          "tools/nx-angular-generators/generators.json",
          "tools/nx-angular-generators/executors.json",
          "tools/nx-angular-generators/package.json"
        ]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "tools/nx-angular-generators/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    }
  },
  "tags": []
}
