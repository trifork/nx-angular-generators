{
  "$schema": "http://json-schema.org/schema",
  "cli": "nx",
  "$id": "DataAccess",
  "title": "",
  "type": "object",
  "properties": {
    "domainName": {
      "type": "string",
      "description": "",
      "$default": {
        "$source": "argv",
        "index": 0
      },
      "pattern": "^(?!.*[/])(?!.*shared).*$",
      "x-prompt": "Input the name of domain to which a data-access lib should be added. E.g. 'medicine'. Do not prepend with '/libs/'.\n"
    }
  },
  "required": ["domainName"]
}
